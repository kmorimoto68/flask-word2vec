{
  "env": {
    "browser": true,
    "jquery": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-extra-semi": "off",
    "no-undef": "off"
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/recommended"
  ]
}
